#include <stdio.h>
int main()
{
        int num1=29, num2=21, num3=26;

        if(num1 > num2 && num1 > num3)
                printf("%d is greater than both the numbers",num1);
        else if(num2 > num1 && num2 > num3)
                printf("%d is greater than both the numbers",num2);
        else if(num3 > num1 && num3 > num2)
                printf("%d is greater than both the numbers",num3);
        else
                printf("None of the number is greatest");
        return 0;
}
