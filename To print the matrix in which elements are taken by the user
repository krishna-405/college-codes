#include <stdio.h>

int main() 
{
    int rows, col;
    
    printf("Enter the number of rows and columns: ");
    scanf("%d %d", &rows, &col);

    int m[100][100]; 

    printf("Enter matrix elements:\n");
    for (int i = 0; i < rows; i++) 
    {
        for (int j = 0; j < col; j++) 
        {
            scanf("%d", &m[i][j]);
        }
    }

    printf("The entered matrix is:\n");
    for (int i = 0; i < rows; i++) 
    {
        for (int j = 0; j < col; j++) 
        {
            printf("%d ", m[i][j]);
        }
        printf("\n");
    }

    return 0;
}
