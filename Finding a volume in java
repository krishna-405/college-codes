class Box{
    double height;
    double width;
    double breadth;
    
    void volume()
    {
        //volume = width*height*breadth;
        System.out.print("Volume is ");
        System.out.println(width * height * breadth);
    }
}
class Volume {
    public static void main(String[] args) 
    {
        Box mybox1 = new Box();
        Box mybox2 = new Box();

        mybox1.width = 10;
        mybox1.height = 20;
        mybox1.breadth = 15;
        
        mybox2.width = 3;
        mybox2.height = 6;
        mybox2.breadth = 9;

        mybox1.volume();
        mybox2.volume();
    }
}
